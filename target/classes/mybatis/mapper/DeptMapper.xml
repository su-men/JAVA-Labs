<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.summer.mapper.DeptMapper">

    <select id="queryDeptList" resultType="Department" >
        select * from mybatis.department;
    </select>

    <select id="queryDeptById" resultType="Department">
        select * from mybatis.department where id=#{id}
    </select>


</mapper>